package jp.co.asnew

import jp.co.asnew.chunk.*
import jp.co.asnew.entity.*

rule "sub 1 rule"
//	salience 1
	activation-group "RuleB"
	no-loop false
	lock-on-active true
	agenda-group "agendaSub1"
	when
		$report : Report( volume == 1 )
		$unitCost : UnitCost()
	then
		modify($unitCost) { setVolume($report.getVolume()), setCost(20 * $report.getVolume()) };
	end

